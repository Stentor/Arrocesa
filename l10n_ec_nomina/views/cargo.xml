<odoo>
    <data>
        <record id="view_iess_sectorial_cargo" model="ir.ui.view">
            <field name="name">view_iess_sectorial_cargo</field>
            <field name="model">iess.sectorial.cargo</field>
	    <field name="arch" type="xml">
            <form action="">
                <sheet>
                    <group>
                        <group>
                            <field name="code" />
                            <field name="rama_id"/>
                        </group>
                        <group>
                            <field name="name" />
                            <field name="value"/>
                        </group>
                    </group>
                </sheet>
            </form>
	    </field>
	</record>

    <record id="tree_iess_sectorial_cargo" model="ir.ui.view">
        <field name="name">tree_iess_sectorial_cargo</field>
        <field name="model">iess.sectorial.cargo</field>
        <field name="arch" type="xml">
            <tree string="Cargos">
                <field name="code"/>
                <field name="name"/>
                
            </tree>
        </field>
    </record>
    
    </data>
</odoo>